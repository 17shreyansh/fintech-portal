    "start": "cd backend && npm start",
    "build": "cd frontend && npm run build"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "keywords": [
    "fintech",
    "mern",
    "react",
    "nodejs",
    "mongodb",
    "investment"
  ],
  "author": "FinTech Team",
