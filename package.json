{
  "name": "fintech-app",
  "version": "1.0.0",
  "description": "Complete MERN Stack FinTech Application",
  "scripts": {
    "install-all": "cd backend && npm install && cd ../frontend && npm install",
    "seed": "cd backend && npm run seed",
